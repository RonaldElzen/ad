<Window x:Class="Eindopdracht.QuadraticHashWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Eindopdracht"
        mc:Ignorable="d"
        Title="QuadraticHashWindow" Height="394.652" Width="300">
    <Grid>
        <ListBox x:Name="listBoxHash" HorizontalAlignment="Left" Height="243" VerticalAlignment="Top" Width="272" Margin="10,10,0,0"/>
        <TextBox x:Name="TextboxAddHash" HorizontalAlignment="Left" Height="24" Margin="128,295,0,-50" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonAddToHash" Content="Add" HorizontalAlignment="Left" Margin="34,295,0,-50" VerticalAlignment="Top" Width="76" Click="buttonAddToHash_Click" Height="24"/>
        <TextBox x:Name="textBoxRemoveHash" HorizontalAlignment="Left" Height="24" Margin="128,324,0,-79" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonRemoveFromHash" Content="Remove" HorizontalAlignment="Left" Margin="34,324,0,0" VerticalAlignment="Top" Width="76" Height="24" Click="buttonRemoveFromHash_Click"/>

    </Grid>
</Window>
